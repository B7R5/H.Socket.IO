<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;netstandard2.0</TargetFrameworks>
    <LangVersion>8</LangVersion>
    <Authors>havendv</Authors>
    <Company>havendv</Company>
    <Description>Simple client for Socket.IO, supports the latest version. Includes EngineIoClient and WebSocketClient</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.9.5</Version>
    <PackageProjectUrl>https://github.com/HavenDV/SimpleSocketIoClient</PackageProjectUrl>
    <RepositoryUrl>https://github.com/HavenDV/SimpleSocketIoClient</RepositoryUrl>
    <PackageTags>SocketIO, EngineIO, Socket.IO, Engine.IO, WebSockets, WebSocket</PackageTags>
    <PackageReleaseNotes>Added support of .NET Standard 2.0</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

</Project>
